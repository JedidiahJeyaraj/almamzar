	<section class="breadcrumbs breadcrumbs2 portfolioBG wow fadeInDown" data-wow-duration="0.25s" data-wow-delay="0.45s">
		<div class="boxedeb">
			<div class="container page-block-small">
				<div class="col-md-12 col-sm-12 col-xs-12">
					<h2>{{Gallery}}</h2>
					<span class="line"></span>
					<div class="rightText">
						<a href="{{ path_for('index_page') }}" title="Home">Home</a> / <a href="{{ path_for('gallery') }}" title="Gallery">Gallery</a> / {{imgs.0.name}}
					</div>
				</div>
			</div>
			<!--end-container-->
		</div>
	</section>
