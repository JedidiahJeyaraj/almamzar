		<div class="project-gallery">
			<div class="gallery-heading BGlight page-block-small wow fadeInUp" data-wow-duration="0.50s" data-wow-delay="0.50s">
				<div class="container">
					<div class="col-md-4 col-sm-5 col-xs-12">
						<h1><i class="fa fa-paint-brush squareSmall BGsec"></i>{{imgs.0.name}}</h1></div>

				</div>
			</div>

			<div class="container wow fadeInUp" data-wow-duration="0.50s" data-wow-delay="0.50s">
				<div class="grid two page-block">
					{% for img in imgs %}
					<div class="item" style="width:500px; height:371px; background:transparent">
						<img class="img-responsive" src="..\images\{{img.project_no}}\{{img.img_name}}" alt="" style="width:100%; height:100%;">
						<div class="overlay">
							<a class="zoom" href="..\images\{{img.project_no}}\{{img.img_name}}" data-rel="prettyPhoto[gallery1]" title="{{img.name}}"><i class="fa fa-search-plus square"></i></a>
						</div>
					</div>
					{% endfor %}
				</div>
			</div>

		</div>

	</section>
